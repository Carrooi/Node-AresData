{
	"packages": {
		"tests": {
			"base": "../../",
			"target": "./test/browser/application.js",
			"modules": [
				"./lib/*.js",
				"./test/browser/tests/*.coffee",
				"./test/browser/responses/*.coffee",
				"moment/moment.js",
				"moment/lang/en-gb.js",
				"q",
				"xml2js/*.js",
				"browser-http/lib/*.js",
				"browser-http/Mocks/*.js",
				"browser-http/Helpers.js",
				"events"
			],
			"aliases": {
				"ares-data": "/lib/Ares",
				"ares-data/Validators": "/lib/Validators"
			},
			"run": [
				"/test/browser/tests/Validators",
				"/test/browser/tests/Ares"
			]
		}
	}
}
