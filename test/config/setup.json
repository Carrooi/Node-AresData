{
	"packages": {
		"tests": {
			"base": "../",
			"target": "./test/application.js",
			"modules": [
				"./lib/*.js",
				"./test/tests/*.coffee",
				"./test/responses/*.coffee",
				"moment/moment.js",
				"moment/lang/en-gb.js",
				"q",
				"xml2js/*.js",
				"browser-http/lib/*.js",
				"browser-http/Mocks/*.js",
				"browser-http/Helpers.js",
				"events"
			],
			"aliases": {
				"ares-data": "/lib/Ares",
				"ares-data/Validators": "/lib/Validators"
			},
			"run": [
				"/test/tests/Validators",
				"/test/tests/Ares"
			]
		}
	}
}
